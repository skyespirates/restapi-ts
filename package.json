{
  "name": "restapi-ts",
  "version": "1.0.0",
  "description": "Simple RESTAPI using express and typescript",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "predev": "rimraf server && tsc",
    "dev-build": "tsc -w",
    "dev-run": "nodemon server/index.js",
    "dev": "concurrently npm:dev-*",
    "preserve": "rimraf server && tsc",
    "serve": "concurrently \"tsc -w\" \"nodemon server/index.js\"",
    "skyes": "concurrently \"tsc -w\" \"nodemon server/index.js\"",
    "start": "node server/index.js"
  },
  "keywords": [
    "rest",
    "restapi",
    "express",
    "typescript",
    "mongodb",
    "mongoose"
  ],
  "author": "skyes",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/dotenv": "^8.2.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.0",
    "react-bootstrap": "^2.8.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.1",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.14.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/react-router-bootstrap": "^0.26.0",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1"
  }
}
